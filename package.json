{
  "name": "@jasonsbarr/htdp-image",
  "description": "A JavaScript port of the Racket Image library used in How To Design Programs",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.7.0",
  "main": "./dist/index.cjs",
  "source": "./src/index.js",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.js"
  },
  "module": "./dist/index.esm.js",
  "unpkg": "./dist/index.umd.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier -w ./src",
    "build": "microbundle --external jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jasonsbarr/htdp-image.git"
  },
  "keywords": [
    "images",
    "htdp",
    "canvas"
  ],
  "author": "Jason Barr <jason@jasonsbarr.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jasonsbarr/htdp-image/issues"
  },
  "homepage": "https://github.com/jasonsbarr/htdp-image#readme",
  "devDependencies": {
    "microbundle": "^0.15.1",
    "prettier": "^2.8.8"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "jsdom": "^22.1.0",
    "object-hash": "^3.0.0"
  }
}
